<included>
  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">

    <!-- If if true, may have a problem for windows -->
    <withJansi>false</withJansi>
      <encoder>
        <pattern>
          %date %highlight(%-5level) %cyan(%logger{5}@[%-4.30thread]) - %msg%n
        </pattern>
      </encoder>
  </appender>
</included>
